{% extends 'base.html.twig' %}

{% block title %}UserClient index{% endblock %}

{% block body %}
    <h1>UserClient index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>FirstName</th>
                <th>LastName</th>
                <th>Phone</th>
                <th>Email</th>
                <th>RequestedService</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for user_client in user_clients %}
            <tr>
                <td>{{ user_client.id }}</td>
                <td>{{ user_client.firstName }}</td>
                <td>{{ user_client.lastName }}</td>
                <td>{{ user_client.phone }}</td>
                <td>{{ user_client.email }}</td>
                <td>{{ user_client.requestedService }}</td>
                <td>
                    <a href="{{ path('app_user_client_show', {'id': user_client.id}) }}">show</a>
                    <a href="{{ path('app_user_client_edit', {'id': user_client.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="7">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_user_client_new') }}">Create new</a>
{% endblock %}
